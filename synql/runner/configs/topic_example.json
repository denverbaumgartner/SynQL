{
    "data_path": "local_data/spider/",
    "database_path": "local_data/spider/database",
    "create_statement_path": "../synsql/data/spider_create_statements.json",

    "model": "gpt-4",
    "temperature": 0.7,
    "topic_user_prompt_version": "1.2.0",
    "topic_system_prompt_version": "1.2.0",

    "save_local": true,
    "save_prompt_path": "local_data/generation_seed_spider_topics",
    "save_results_path": "local_data/generation_results_spider_topics.jsonl",

    "max_requests_per_minute": 10000,
    "max_tokens_per_minute": 300000,
    "safety_scalar": 0.5,
    "request_url": "https://api.openai.com/v1/chat/completions",
    "token_encoding_name": "cl100k_base",
    "max_attempts": 5,

    "run_generation": true
}